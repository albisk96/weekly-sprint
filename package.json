    "cookie": "^0.4.0",
    "express": "^4.17.1",
    "express-async-wrap": "^1.0.0",
    "express-sslify": "^1.2.0",
    "express-validator": "^6.3.1",
    "mathjs": "^6.6.2",
    "mongoose": "^5.8.10",
    "node-cron": "^2.0.3",
    "nodemon": "^2.0.2",
    "react-datepicker": "^2.14.1",
    "react-router-dom": "^5.2.0",
    "request": "^2.88.0"
